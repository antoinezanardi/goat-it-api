meta {
  name: Modify Question Theme By Id
  type: http
  seq: 5
}

patch {
  url: {{baseUrl}}/admin/question-themes/:id
  body: json
  auth: inherit
}

params:path {
  id: {{questionThemeId}}
}

body:json {
  {
    "aliases": {
      "en": ["Movies", "Series"],
      "fr": ["lol", "mdr"]
    }
  }
}

settings {
  encodeUrl: true
  timeout: inherit
  followRedirects: true
  maxRedirects: 5
}
