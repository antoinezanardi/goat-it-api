{
  "name": "goat-it-api",
  "version": "1.11.0",
  "description": "ðŸ Goat It API is a fast and efficient API built with NestJS and Fastify, designed to provide seamless backend services for Goat It applications.",
  "private": true,
  "keywords": [
    "goat-it",
    "nestjs",
    "fastify",
    "mongodb",
    "api",
    "typescript",
    "backend"
  ],
  "homepage": "https://github.com/antoinezanardi/goat-it-api#readme",
  "bugs": {
    "url": "https://github.com/antoinezanardi/goat-it-api/issues",
    "email": "antoine.zanardi@epitech.eu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoinezanardi/goat-it-api.git",
    "branch": "main"
  },
  "license": "MIT",
  "author": "Antoine ZANARDI",
  "contributors": [
    {
      "name": "Antoine ZANARDI",
      "email": "antoine.zanardi@epitech.eu",
      "url": "https://antoinezanardi.fr"
    }
  ],
  "maintainers": [
    {
      "name": "Antoine ZANARDI",
      "email": "antoine.zanardi@epitech.eu",
      "url": "https://antoinezanardi.fr"
    }
  ],
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/antoinezanardi"
  },
  "type": "commonjs",
  "main": "dist/main.js",
  "readme": "README.md",
  "engines": {
    "node": ">=25.6.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "scripts": {
    "prepare": "scripts/post-install-prepare.sh",
    "build": "nest build --config configs/nest/nest-cli.config.json",
    "typecheck": "tsgo -b --clean && tsgo -b --noEmit",
    "start": "nest start --config configs/nest/nest-cli.config.json",
    "start:dev": "nest start --watch --config configs/nest/nest-cli.config.json",
    "start:debug": "nest start --debug --watch --config configs/nest/nest-cli.config.json",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:prod:test": "cross-env NODE_ENV=test node dist/main",
    "lint": "pnpm run lint:oxlint && pnpm run lint:eslint",
    "lint:fix": "pnpm run lint:oxlint:fix && pnpm run lint:eslint:fix",
    "lint:staged:fix": "pnpm run lint:oxlint:staged:fix && pnpm run lint:eslint:staged:fix",
    "lint:eslint": "eslint --config eslint.config.ts .",
    "lint:eslint:fix": "eslint --config eslint.config.ts --fix .",
    "lint:eslint:staged:fix": "lint-staged -c configs/lint-staged/.lintstagedrc.eslint.json",
    "lint:eslint:inspect-config": "config-inspector",
    "lint:oxlint": "oxlint --type-aware -c configs/oxlint/oxlint.config.jsonc",
    "lint:oxlint:fix": "oxlint --type-aware -c configs/oxlint/oxlint.config.jsonc --fix --fix-suggestions",
    "lint:oxlint:staged:fix": "lint-staged -c configs/lint-staged/.lintstagedrc.oxlint.json",
    "test": "pnpm run test:unit:cov && pnpm run test:mutation && pnpm run test:acceptance",
    "test:unit": "cross-env NODE_ENV=test vitest --config configs/vitest/vitest.config.ts",
    "test:unit:watch": "cross-env NODE_ENV=test vitest --config configs/vitest/vitest.config.ts --watch",
    "test:unit:cov": "cross-env NODE_ENV=test vitest --config configs/vitest/vitest.config.ts --coverage",
    "test:unit:staged": "lint-staged -c configs/lint-staged/.lintstagedrc.unit-tests.json",
    "test:mutation": "cross-env NODE_ENV=test rimraf tests/mutation/coverage && stryker run configs/stryker/stryker.config.mjs",
    "test:mutation:force": "cross-env NODE_ENV=test rimraf tests/mutation/incremental tests/mutation/coverage && stryker run configs/stryker/stryker.config.mjs --force",
    "test:mutation:ci": "cross-env NODE_ENV=test rimraf tests/mutation/coverage && stryker run configs/stryker/stryker-ci.config.mjs",
    "test:acceptance": "cross-env NODE_ENV=test TSX_TSCONFIG_PATH='configs/typescript/tsconfig.acceptance.json' cucumber-js --config configs/cucumber/cucumber.json; EXIT_CODE_CUCUMBER=$?; pnpm run test:cucumber:html-report; exit $EXIT_CODE_CUCUMBER",
    "test:acceptance:skip-build": "cross-env SKIP_BUILD=true pnpm run test:acceptance",
    "test:cucumber:html-report": "tsx --tsconfig 'configs/typescript/tsconfig.acceptance.json' tests/acceptance/plugins/html-reporter/cucumber-html-reporter.ts",
    "script:create-branch": "scripts/create-git-branch.sh",
    "script:create-pull-request": "scripts/create-pull-request.sh",
    "script:create-release-changelog": "scripts/create-release-changelog.sh",
    "script:clean-pr-description": "scripts/clean-pr-description.sh",
    "validate:branch-name": "(cd configs/validate-branch-name && validate-branch-name)",
    "docker:build": "docker buildx build -t goat-it-api .",
    "docker:build:linux/arm64": "docker buildx build --platform linux/arm64 -t goat-it-api .",
    "docker:build:linux/amd64": "docker buildx build --platform linux/amd64 -t goat-it-api .",
    "docker:acceptance-tests:start": "docker compose -f docker/goat-it-api-acceptance-tests/docker-compose.yml up -d",
    "docker:acceptance-tests:stop": "docker compose -f docker/goat-it-api-acceptance-tests/docker-compose.yml down",
    "docker:acceptance-tests:reset": "npm run docker:acceptance-tests:stop && npm run docker:acceptance-tests:start"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.4.1",
    "@cucumber/cucumber": "^12.6.0",
    "@eslint/config-inspector": "^1.4.2",
    "@faker-js/faker": "^10.3.0",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.13",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stryker-mutator/core": "^9.5.1",
    "@stryker-mutator/typescript-checker": "^9.5.1",
    "@stryker-mutator/vitest-runner": "^9.5.1",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@swc/cli": "^0.8.0",
    "@swc/core": "^1.15.11",
    "@types/node": "^24.10.12",
    "@types/validator": "^13.15.10",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@typescript/native-preview": "7.0.0-dev.20260201.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.6",
    "commitlint": "^20.4.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cucumber-html-reporter": "^7.2.0",
    "dotenv": "^17.2.4",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-oxlint": "1.43.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "expect": "^30.2.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "ofetch": "^1.5.1",
    "oxlint": "^1.43.0",
    "oxlint-tsgolint": "^0.11.5",
    "pino-pretty": "^13.1.3",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.3",
    "semantic-release-export-data": "^1.2.0",
    "tsx": "^4.21.0",
    "type-fest": "^5.4.4",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.9",
    "validate-branch-name": "^1.3.2",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.29.2",
  "dependencies": {
    "@fastify/static": "^9.0.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.13",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.13",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/platform-fastify": "^11.1.13",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/terminus": "^11.0.0",
    "axios": "^1.13.5",
    "cross-env": "^10.1.0",
    "fastify": "^5.7.4",
    "mongoose": "^9.1.6",
    "nestjs-pino": "^4.5.0",
    "nestjs-zod": "^5.1.1",
    "radashi": "^12.7.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "validator": "^13.15.26",
    "zod": "^4.3.6"
  }
}
