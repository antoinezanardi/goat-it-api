{
  "name": "goat-it-api",
  "version": "1.4.0",
  "description": "ðŸ Goat It API is a fast and efficient API built with NestJS and Fastify, designed to provide seamless backend services for Goat It applications.",
  "private": true,
  "keywords": [
    "goat-it",
    "nestjs",
    "fastify",
    "mongodb",
    "api",
    "typescript",
    "backend"
  ],
  "homepage": "https://github.com/antoinezanardi/goat-it-api#readme",
  "bugs": {
    "url": "https://github.com/antoinezanardi/goat-it-api/issues",
    "email": "antoine.zanardi@epitech.eu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoinezanardi/goat-it-api.git",
    "branch": "main"
  },
  "license": "MIT",
  "author": "Antoine ZANARDI",
  "contributors": [
    {
      "name": "Antoine ZANARDI",
      "email": "antoine.zanardi@epitech.eu",
      "url": "https://antoinezanardi.fr"
    }
  ],
  "maintainers": [
    {
      "name": "Antoine ZANARDI",
      "email": "antoine.zanardi@epitech.eu",
      "url": "https://antoinezanardi.fr"
    }
  ],
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/antoinezanardi"
  },
  "type": "commonjs",
  "main": "dist/main.js",
  "readme": "README.md",
  "engines": {
    "node": ">=24.11.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "scripts": {
    "prepare": "scripts/post-install-prepare.sh",
    "build": "nest build --config configs/nest/nest-cli.config.json",
    "typecheck": "tsc -b --clean && tsc -b --noEmit",
    "start": "nest start --config configs/nest/nest-cli.config.json",
    "start:dev": "nest start --watch --config configs/nest/nest-cli.config.json",
    "start:debug": "nest start --debug --watch --config configs/nest/nest-cli.config.json",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "npx eslint --config eslint.config.ts .",
    "lint:fix": "npx eslint --config eslint.config.ts --fix .",
    "lint:staged:fix": "lint-staged -c configs/lint-staged/.lintstagedrc.eslint.json",
    "lint:inspect-config": "npx @eslint/config-inspector",
    "test": "npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test vitest --config configs/vitest/vitest.config.ts",
    "test:unit:watch": "cross-env NODE_ENV=test vitest --config configs/vitest/vitest.config.ts --watch",
    "test:unit:cov": "cross-env NODE_ENV=test vitest --config configs/vitest/vitest.config.ts --coverage",
    "test:unit:staged": "lint-staged -c configs/lint-staged/.lintstagedrc.unit-tests.json",
    "test:mutation": "cross-env NODE_ENV=test rimraf tests/mutation/coverage && stryker run configs/stryker/stryker.config.mjs",
    "test:mutation:force": "cross-env NODE_ENV=test rimraf tests/mutation/incremental tests/mutation/coverage && stryker run configs/stryker/stryker.config.mjs --force",
    "test:mutation:ci": "cross-env NODE_ENV=test rimraf tests/mutation/coverage && stryker run configs/stryker/stryker-ci.config.mjs",
    "test:acceptance": "cross-env NODE_ENV=test TSX_TSCONFIG_PATH='configs/typescript/tsconfig.acceptance.json' cucumber-js --config configs/cucumber/cucumber.json; EXIT_CODE_CUCUMBER=$?; npm run test:cucumber:html-report; exit $EXIT_CODE_CUCUMBER",
    "test:cucumber:html-report": "tsx --tsconfig 'configs/typescript/tsconfig.acceptance.json' tests/acceptance/plugins/html-reporter/cucumber-html-reporter.ts",
    "script:create-branch": "scripts/create-git-branch.sh",
    "script:create-pull-request": "scripts/create-pull-request.sh",
    "script:create-release-changelog": "scripts/create-release-changelog.sh",
    "validate:branch-name": "(cd configs/validate-branch-name && validate-branch-name)",
    "docker:build": "docker buildx build -t goat-it-api .",
    "docker:build:linux/arm64": "docker buildx build --platform linux/arm64 -t goat-it-api .",
    "docker:build:linux/amd64": "docker buildx build --platform linux/amd64 -t goat-it-api ."
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@cucumber/cucumber": "^12.2.0",
    "@eslint/config-inspector": "^1.3.0",
    "@faker-js/faker": "^10.1.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stryker-mutator/core": "^9.2.0",
    "@stryker-mutator/typescript-checker": "^9.2.0",
    "@stryker-mutator/vitest-runner": "^9.2.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.21",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/eslint-plugin": "^1.3.26",
    "commitlint": "^20.1.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.1.0",
    "cucumber-html-reporter": "^7.2.0",
    "eslint": "^9.38.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "expect": "^30.2.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.6",
    "ofetch": "^1.5.0",
    "pino-pretty": "^13.1.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^25.0.1",
    "semantic-release-export-data": "^1.2.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.8",
    "validate-branch-name": "^1.3.2",
    "vitest": "^4.0.4",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-fastify": "^11.1.8",
    "nestjs-pino": "^4.4.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  }
}
